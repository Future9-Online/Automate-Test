            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">// Get the new token
String newToken = vars.get(&quot;token&quot;)

// Get the current token list or start new
String currentList = props.get(&quot;sharedTokens&quot;)
if (currentList == null) {
    currentList = &quot;&quot;
}

// Append new token (using &apos;|&apos; as delimiter)
if (!currentList.isEmpty()) {
    currentList += &quot;|&quot; + newToken
} else {
    currentList = newToken
}

// Save it back
props.put(&quot;sharedTokens&quot;, currentList)
</stringProp>
